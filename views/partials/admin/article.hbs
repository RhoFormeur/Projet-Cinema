{{!-- 1 Article --}}
<tr>
    <td scope="row">{{this.titre}}</td>
    <td class="d-flex justify-content-end gap-1">
        <button type="button" class="btn btn-warning" data-bs-toggle="collapse" data-bs-target="#collapseOpen{{this.id}}"
            aria-expanded="false" aria-controls="collapseOpen{{this.id}}"><i class="fa-regular fa-eye"></i></button>

        <button type="button" class="btn btn-warning"><i class="fa-regular fa-pen-to-square"></i></button>
        <button type="button" class="btn btn-danger"><i class="fa-regular fa-trash-can"></i></button>
    </td>
    <div class="collapse" id="collapseOpen{{this.id}}">
        <div class="card card-body">
            Some placeholder content for the collapse component. This panel is hidden by default but revealed when the
            user activates the relevant trigger.
        </div>
    </div>
</tr>